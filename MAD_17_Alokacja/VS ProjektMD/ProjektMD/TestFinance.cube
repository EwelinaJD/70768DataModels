      <Commands>
        <Command>
          <Text>/*
The CALCULATE command controls the aggregation of leaf cells in the cube.
If the CALCULATE command is deleted or modified, the data within the cube is affected.
You should edit this command only if you manually specify how the cube is aggregated.
*/
CALCULATE;   

--  Measures (Empty, Visible)
-----------------------------------------------------------

    CREATE MEMBER CURRENTCUBE.[Measures].[AmountTgt]
    AS NULL, 
    FORMAT_STRING = "#,##0.00", 
    VISIBLE = 1 ,  
    ASSOCIATED_MEASURE_GROUP = 'Fact Actuals';   

    CREATE MEMBER CURRENTCUBE.[Measures].[AmountBdg]
    AS NULL, 
    FORMAT_STRING   = "#,##0.00", 
    VISIBLE = 1 ,  
    ASSOCIATED_MEASURE_GROUP = 'Fact Actuals';   

--  Measures (Diff&amp;Ratio, Visible)
-----------------------------------------------------------

    CREATE MEMBER CURRENTCUBE.[Measures].[ActToTgt]
    AS [Measures].[Amount] - [Measures].[AmountTgt], 
    FORMAT_STRING = "#,##0.00", 
    FORE_COLOR= IIF(    [Measures].[ActToTgt] &gt;= 0,
                        RGB(0,100,0), 
                        RGB(255,0,0)
                        ),
    VISIBLE = 1 ,  
    ASSOCIATED_MEASURE_GROUP = 'Fact Actuals'  ;   

    CREATE MEMBER CURRENTCUBE.[Measures].[ActToTgt(%)]
    AS ([Measures].[Amount] - [Measures].[AmountTgt]) / [Measures].[AmountTgt], 
    FORMAT_STRING = "#,##0.00%", 
    FORE_COLOR= IIF(    [Measures].[ActToTgt(%)] &gt;= 0,
                        RGB(0,100,0), 
                        RGB(255,0,0)
                        ),
    VISIBLE = 1 ,  
    ASSOCIATED_MEASURE_GROUP = 'Fact Actuals';   


--  Measures (Intermediate, Hidden)
-----------------------------------------------------------

    CREATE MEMBER CURRENTCUBE.[Measures].[_MembersInYear]
    AS
        COUNT(
	    DESCENDANTS(	ANCESTOR(	[Date].[YQM].CURRENTMEMBER,
	    							[Date].[YQM].[Year Key]
	    							),
	    				[Date].[YQM].CURRENTMEMBER.LEVEL
	    				)
	    )
    , 
    FORMAT_STRING = "#,##0.00",
    VISIBLE = 0;  

    CREATE MEMBER CURRENTCUBE.[Measures].[_AmountTgtCurrentYear]
    AS
	    (	[Measures].[_AmountTgt],
		    ANCESTOR(	[Date].[YQM].CURRENTMEMBER,
				        [Date].[YQM].[Year Key]
				        )
        )
    ,
    FORMAT_STRING = "#,##0.00",
    VISIBLE = 0; 

    CREATE MEMBER CURRENTCUBE.[Measures].[_AmountParallelPeriodInYear]
    AS
	    (	[Measures].[Amount],
		    ParallelPeriod                               
				    (	[Date].[YQM].[Year Key], 
				    1,  
				    [Date].[YQM].CURRENTMEMBER   
				    ) 
	    )
    ,
    FORMAT_STRING = "#,##0.00",
    VISIBLE = 0;  

    CREATE MEMBER CURRENTCUBE.[Measures].[_AmountLastYear]
    AS
	    (	[Measures].[Amount],
	    ANCESTOR(	[Date].[YQM].CURRENTMEMBER,
				    [Date].[YQM].[Year Key]
				    ).LAG(1)
	    )	
    ,
    FORMAT_STRING = "#,##0.00",
    VISIBLE = 0;  

----------------------------------------------------------

    CREATE MEMBER CURRENTCUBE.[Measures].[_AmountTgtDvd]
    AS [Measures].[_AmountTgtCurrentYear] / [Measures].[_MembersInYear]
    , 
    FORMAT_STRING = "#,##0.00",
    VISIBLE = 0;   

    CREATE MEMBER CURRENTCUBE.[Measures].[_AmountTgtAloc]
    AS
	    [Measures].[_AmountTgtCurrentYear]  * 
	    (   [Measures].[_AmountParallelPeriodInYear]    /   [Measures].[_AmountLastYear])
    ,
    FORMAT_STRING = "#,##0.00",
    VISIBLE = 0;   

--  SCOPE
-----------------------------------------------------------

    SCOPE(  [Measures].[AmountTgt]);  
   
        SCOPE(  DESCENDANTS([Date].[YQM],[Date].[YQM].[Year Key],AFTER));  
        
            THIS = IIF(     IsAncestor(     [Date].[YQM].[Year Key].&amp;[2016-01-01T00:00:00]
                                        ,   [Date].[YQM].CURRENTMEMBER
                                        )
                        ,   [Measures].[_AmountTgtDvd]
